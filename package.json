{
  "name": "readme-metric-reporter",
  "description": "Collect and publish API usage metrics to README.io",
  "version": "0.0.0",
  "homepage": "https://github.com/igrek8/readme-metric-reporter#readme",
  "author": "Igor Korchagin (https://github.com/igrek8)",
  "license": "MIT",
  "keywords": [
    "metrics",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/igrek8/readme-metric-reporter"
  },
  "bugs": {
    "url": "https://github.com/igrek8/readme-metric-reporter/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "runkitExampleFilename": "./docs/runkit.js",
  "files": [
    "./dist",
    "./docs/runkit.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "compile": "tsc --project ./tsconfig.build.json",
    "watch": "tsc --watch --project ./tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "lint": "eslint \"**/*\"",
    "format": "prettier --check \"**/*\""
  },
  "devDependencies": {
    "@types/jest": "29.0.3",
    "@types/node": "16.11.59",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "await-lock": "2.2.2",
    "tslib": "2.4.0",
    "utility-types": "3.10.0"
  }
}
